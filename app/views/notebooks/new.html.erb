<%= form_with scope: :search_notebook, url: notebooks_path, local: true do |f| %>
  <div class="input-group p-3">
    <%= f.text_field :title, placeholder: "Title", class: "form-control" %>
    <div class="input-group-btn">
      <%= f.submit "Create", class: 'btn btn-info glyphicon glyphicon-search' %>
    </div>
  </div>
<% end %>
<%= link_to 'Back', notebooks_path, class: 'btn btn-primary' %>
